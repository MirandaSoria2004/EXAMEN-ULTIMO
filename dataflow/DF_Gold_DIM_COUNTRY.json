{
	"name": "DF_Gold_DIM_COUNTRY",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SQL_Silver_Clientes",
						"type": "DatasetReference"
					},
					"name": "SourceCountry"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_SQL_Gold_DIM_COUNTRY",
						"type": "DatasetReference"
					},
					"name": "SinkDimCountry"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          cst_country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT cst_country FROM [silver].[clientes] WHERE cst_country IS NOT NULL',",
				"     format: 'query') ~> SourceCountry",
				"SourceCountry select(mapColumn(",
				"          CNTRY = cst_country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ID_CNTRY as integer,",
				"          CNTRY as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          CNTRY",
				"     )) ~> SinkDimCountry"
			]
		}
	}
}