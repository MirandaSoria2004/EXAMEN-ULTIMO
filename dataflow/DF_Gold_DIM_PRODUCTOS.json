{
	"name": "DF_Gold_DIM_PRODUCTOS",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SQL_Silver_Productos",
						"type": "DatasetReference"
					},
					"name": "SourceProductos"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_Gold_DIM_CATEGORIA",
						"type": "DatasetReference"
					},
					"name": "SourceDimCategoria"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_SQL_Gold_DIM_PRODUCTOS",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "LookupCategoria"
				},
				{
					"name": "select1"
				},
				{
					"name": "EliminarDuplicados"
				}
			],
			"scriptLines": [
				"source(output(",
				"          prd_id as integer,",
				"          prd_key as string,",
				"          prd_nm as string,",
				"          prd_cost as decimal(10,2),",
				"          prd_line as string,",
				"          prd_cat as string,",
				"          prd_subcat as string,",
				"          prd_maintenance as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT * FROM [silver].[productos]',",
				"     format: 'query') ~> SourceProductos",
				"source(output(",
				"          ID_CAT as integer,",
				"          CAT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT ID_CAT, CAT FROM [GOLD].[DIM_CATEGORIA]',",
				"     format: 'query') ~> SourceDimCategoria",
				"EliminarDuplicados, SourceDimCategoria lookup(prd_cat == CAT,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> LookupCategoria",
				"LookupCategoria select(mapColumn(",
				"          PRD_KEY = prd_key,",
				"          PRD_NM = prd_nm,",
				"          PRD_COST = prd_cost,",
				"          PRD_LINE = prd_line,",
				"          MAINTENANCE = prd_maintenance,",
				"          ID_CAT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"SourceProductos aggregate(groupBy(prd_key),",
				"     prd_id = first(prd_id),",
				"          prd_nm = first(prd_nm),",
				"          prd_cost = first(prd_cost),",
				"          prd_line = first(prd_line),",
				"          prd_cat = first(prd_cat),",
				"          prd_subcat = first(prd_subcat),",
				"          prd_maintenance = first(prd_maintenance)) ~> EliminarDuplicados",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PRD_KEY as string,",
				"          PRD_NM as string,",
				"          PRD_COST as decimal(10,2),",
				"          PRD_LINE as string,",
				"          ID_CAT as integer,",
				"          MAINTENANCE as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          PRD_KEY,",
				"          PRD_NM,",
				"          PRD_COST,",
				"          PRD_LINE,",
				"          ID_CAT,",
				"          MAINTENANCE",
				"     )) ~> sink1"
			]
		}
	}
}